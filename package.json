{
  "name": "safesurfers",
  "displayName": "SafeSurfers",
  "version": "0.0.1",
  "description": "A real-time, AI-driven security sentinel for browser extension integrity.",
  "author": "Safe Surfers",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "package": "plasmo package"
  },
  "dependencies": {
    "esprima": "^4.0.1",
    "plasmo": "0.90.5",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.1.1",
    "@types/chrome": "0.0.258",
    "@types/esprima": "^4.0.6",
    "@types/node": "20.11.5",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "prettier": "3.2.4",
    "typescript": "5.3.3"
  },
  "plasmo": {
    "popup": {
      "filename": "popup.tsx"
    },
    "background": {
      "service_worker": "background.ts"
    }
  },
  "manifest": {
    "host_permissions": [
      "<all_urls>"
    ],
    "permissions": [
      "storage",
      "webRequest",
      "webNavigation",
      "scripting",
      "tabs",
      "management"
    ]
  }
}